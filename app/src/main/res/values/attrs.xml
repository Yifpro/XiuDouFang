<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SimpleEditTextView">
        <attr name="setv_icon" format="reference" />
        <attr name="setv_hint" format="string|reference" />
        <attr name="setv_value" format="string|reference" />
        <attr name="setv_input_type">
            <enum name="TYPE_NULL" value="0" />
        </attr>
        <attr name="setv_is_password" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SimpleToolbar">
        <attr name="toolbar_title" format="string|reference" />
        <attr name="left_icon" format="reference" />
        <attr name="right_icon" format="reference" />
        <attr name="right_text" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="SearchInfoView">
        <attr name="isEditable" format="boolean" />
        <attr name="siv_key" format="string|reference" />
        <attr name="siv_value" format="string|reference" />
        <attr name="siv_hint" format="string|reference" />
        <attr name="siv_is_show_next" format="boolean" />
    </declare-styleable>
</resources>